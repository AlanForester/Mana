<!DOCTYPE html>
<html lang="en">
  <%= render "head.html", conn: @conn %>
  <body class="off-canvas-sidebar">
    <script src="<%= Routes.static_path(@conn, "/js/jquery.min.js") %>"></script>
    <script src="<%= Routes.static_path(@conn, "/js/popper.min.js") %>"></script>
    <script src="<%= Routes.static_path(@conn, "/js/bootstrap-material-design.min.js") %>"></script>
    <script src="<%= Routes.static_path(@conn, "/js/plugins/bootstrap-notify.js") %>"></script>
    <script src="<%= Routes.static_path(@conn, "/js/material-dashboard.js?v=2.1.2") %>" type="text/javascript"></script>
    <script src="<%= Routes.static_path(@conn, "/js/functions.js") %>" type="text/javascript"></script>
    <div class="wrapper wrapper-full-page">
      <%= render "alerts.html", conn: @conn %>
      <div class="page-header login-page header-filter" filter-color="black" style="background-image: url('<%= Routes.static_path(@conn, "/images/login.jpg") %>'); background-size: cover; background-position: top center;">s
        <%= @inner_content %>
      </div>
    </div>
    <script src="<%= Routes.static_path(@conn, "/js/login.js") %>"></script>
    <script>
    $(document).ready(function() {
      md.checkFullPageBackgroundImage();
      setTimeout(function() {
        // after 1000 ms we add the class animated to the login/register card
        $('.card').removeClass('card-hidden');
      }, 700);
    });
    </script>
  </body>
</html>
